{
  "expo": {
    "name": "spotify_playlist_maker",
    "slug": "spotify_playlist_maker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo/swipifylogoSplash.png",
    "userInterfaceStyle": "automatic",
    "scheme": "spotify_playlist_maker",
    "extra": {
      "spotifyRedirectUri": "spotify_playlist_maker://callback"
    },
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#2B2B2B"
    },
    "ios": {
      "userInterfaceStyle": "automatic",
      "supportsTablet": true
    },
    "android": {
      "userInterfaceStyle": "automatic",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2B2B2B"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}
